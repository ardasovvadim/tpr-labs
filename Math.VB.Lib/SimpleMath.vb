Public Class SimpleMath
    Function Add(a As Double, b As Double) As Double
        Return a + b
    End Function

    Function Substract(a As Double, b As Double) As Double
        Return a - b
    End Function

    Function Multiply(a As Double, b As Double) As Double
        Return a * b
    End Function

    Function Divide(a As Double, b As Double) As Double
        Return a / b
    End Function

    Function Sin(a As Double) As Double
        Return System.Math.Sin(a)
    End Function

    Function Sinh(a As Double) As Double
        Return System.Math.Sinh(a)
    End Function

    Function Cos(a As Double) As Double
        Return System.Math.Cos(a)
    End Function

    Function Cosh(a As Double) As Double
        Return System.Math.Cosh(a)
    End Function

    Function Tan(a As Double) As Double
        Return System.Math.Tan(a)
    End Function

    Function Tanh(a As Double) As Double
        Return System.Math.Tanh(a)
    End Function

End Class
